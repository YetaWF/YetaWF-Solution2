{
  "rules": {
    "array-type": false,
    "arrow-parens": true,
    "arrow-return-shorthand": false,
    "ban": [
      true,
      [
        "_",
        "extend"
      ],
      [
        "_",
        "isNull"
      ],
      [
        "_",
        "isDefined"
      ]
    ],
    "ban-comma-operator": true,
    "callable-types": false,
    "class-name": true,
    "comment-format": [
      false
    ],
    "curly": false,
    "eofline": false,
    "forin": true,
    "indent": [
      true,
      "spaces",
      4
    ],
    "interface-name": [
      false
    ],
    "jsdoc-format": true,
    "label-position": true,
    "max-line-length": [
      false
    ],
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-super": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": false,
    "no-empty": false,
    "no-eval": true,
    "no-invalid-template-strings": true,
    "no-invalid-this": true,
    "no-sparse-arrays": true,
    "no-string-literal": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unnecessary-initializer": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "number-literal-format": true,
    "object-literal-key-quotes": false,
    "one-line": [
      false
    ],
    "only-arrow-functions": true,
    "prefer-const": false,
    "prefer-for-of": true,
    "quotemark": [
      true,
      "double"
    ],
    "radix": true,
    "semicolon": [
      true,
      "always"
    ],
    "switch-final-break": false,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"
    ],
    "typedef-whitespace": [
      false
    ],
    "use-isnan": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "whitespace": [
      false,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}